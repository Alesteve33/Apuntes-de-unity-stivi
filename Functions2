using UnityEngine;
using System.Collections;


public class Returning : MonoBehaviour {

  
    void Start (){
    
        float LuckyNumber = RandomNumber();
        print("Your lucky number is " + LuckyNumber);
        
    }
    
    float RandomNumber();
 
    Float number = Random.Range(0, 101);
    return number;
    
    }
 }
