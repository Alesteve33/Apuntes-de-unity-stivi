using UnityEngine;
using System.Collections;


public class Enemy : MonoBehaviour {

    Player playerClass;
  
    void Start (){
        playerClass = GameObject.Find("PlayerObject").GetComponent<Player>();
    }

    void Update (){
    
        if (Input.GetKeyDown(Keycode.Space)){
    
            playerClass.TakeDamage(5f);
   
        }
    
    }
 
 }
  
